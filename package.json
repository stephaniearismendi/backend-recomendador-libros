    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "express-request-id": "^3.0.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "p-retry": "^7.0.0",
    "pino": "^9.9.0",
    "pino-http": "^10.5.0",
    "pino-pretty": "^13.1.1",
    "prisma": "^6.10.0"
  },
  "devDependencies": {
    "cross-env": "^10.0.0",
